     <p id="7112" fn="Pieter Jacob" sn="Henning" g="M" gen="a1.b6.c4.d2.e5." fid="6792" mid="7107" cn="5" dob="1869-9-9" pob="Reddersburg" doc="1870-10-16" poc="Geref K Reddersburg" dod="1937-4-17" pod="Primrose" ts="2013-08-26 13:48:26">
