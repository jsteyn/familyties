     <p id="5093" fn="Frans Engelbertus" sn="Henning" g="M" gen="a1.b5.c4.d4.e2.f8.g2." fid="5032" mid="5091" cn="2" dob="1935-5-16" ts="2013-08-26 13:48:24" />
