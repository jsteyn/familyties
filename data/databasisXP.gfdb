     <p id="1046" fn="Gerhardus Francois" sn="Henning" g="M" gen="a1.b7.c5.d1.e1." fid="564" mid="1043" cn="1" dob="1878-6-16" pob="Ezelshoek, Middelburg Kaap" doc="1878-8-25" poc="Cradock" dod="1918-11-1" pod="Middelburg, Kaap" ts="2013-08-26 13:48:21">
