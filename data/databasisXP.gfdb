     <p id="5499" fn="Pieter Frederik" sn="Henning" g="M" gen="a1.b5.c5.d5.e6." fid="4827" mid="4284" cn="6" dob="1882-10-18" doc="1883-1-28" poc="Geref K Potchefstroom" ts="2013-08-26 13:48:24" />
